<p id="notice"><%= notice %></p>


<%@subs_list = @word.subs_as_objects%>

<%@subs = @word.percentage_each_sub%>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subs.each do |sub| %>
      <tr>
        <td><%=sub[0]%></td>
        <td><%=sub[1].round(2)%> % </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%@word.word_by_driver_and_sub(@subs_list[0].driver)%>




