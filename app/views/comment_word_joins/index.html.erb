<p id="notice"><%= notice %></p>

<h1>Comment Word Joins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_word_joins.each do |comment_word_join| %>
      <tr>
        <td><%= link_to 'Show', comment_word_join %></td>
        <td><%= link_to 'Edit', edit_comment_word_join_path(comment_word_join) %></td>
        <td><%= link_to 'Destroy', comment_word_join, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

