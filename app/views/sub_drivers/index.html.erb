<p id="notice"><%= notice %></p>

<h1>Sub Drivers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sub_drivers.each do |sub_driver| %>
      <tr>
        <td><%= link_to 'Show', sub_driver %></td>
        <td><%= link_to 'Edit', edit_sub_driver_path(sub_driver) %></td>
        <td><%= link_to 'Destroy', sub_driver, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub Driver', new_sub_driver_path %>
